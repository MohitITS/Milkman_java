	<queryString>
		<![CDATA[SELECT
     producer.`ID` AS producer_ID,
     producer.`pro_name` AS producer_pro_name,
     producer.`m_name` AS producer_m_name,
     producer.`addr` AS producer_addr,
     producer.`contact_no` AS producer_contact_no,
     producer.`member` AS producer_member,
     producer.`rtc_no` AS producer_rtc_no,
     producer.`comission` AS producer_comission,
     producer.`ribet` AS producer_ribet,
     producer.`share_amt` AS producer_share_amt,
     producer.`chiling_amt` AS producer_chiling_amt,
     producer.`DateAdded` AS producer_DateAdded,
     producer.`AddedByFK` AS producer_AddedByFK,
     producer.`DateModified` AS producer_DateModified,
     producer.`LastUserFK` AS producer_LastUserFK,
     producer.`Locked` AS producer_Locked,
     producer.`producergroupid` AS producer_producergroupid,
     producer.`legerType` AS producer_legerType,
     producer.`lgr_gr_id` AS producer_lgr_gr_id,
     producer.`opn_bal` AS producer_opn_bal,
     producer.`cr_dr_ID` AS producer_cr_dr_ID,
     producer.`perc` AS producer_perc,
     producer.`bankid` AS producer_bankid,
     producer.`acno` AS producer_acno,
     producer.`veh_exp` AS producer_veh_exp,
     producergroup.`ID` AS producergroup_ID,
     producergroup.`grname` AS producergroup_grname,
     config.`ID` AS config_ID,
     config.`firmname` AS config_firmname,
     config.`address` AS config_address,
     config.`contatno` AS config_contatno,
     config.`swcopycode` AS config_swcopycode,
     config.`refno` AS config_refno,
     config.`licenceno` AS config_licenceno,
     config.`instalationdate` AS config_instalationdate,
     config.`plantorcenter` AS config_plantorcenter,
     config.`regpersonname` AS config_regpersonname,
     config.`curryearfrom` AS config_curryearfrom,
     config.`curryearto` AS config_curryearto,
     config.`currdate` AS config_currdate,
     config.`firmregno` AS config_firmregno,
     config.`emailadd` AS config_emailadd,
     config.`swlang` AS config_swlang
FROM
     `producer` producer INNER JOIN `producergroup` producergroup ON producer.`producergroupid` = producergroup.`ID`,
     `config` config
Order By
     producer.producergroupid, producer.prod_id]]>
	</queryString>

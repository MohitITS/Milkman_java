	<queryString>
		<![CDATA[SELECT
     dailytrn.`ID` AS dailytrn_ID,
     dailytrn.`trndate` AS dailytrn_trndate,
     dailytrn.`voucherid` AS dailytrn_voucherid,
     dailytrn.`chalanno` AS dailytrn_chalanno,
     dailytrn.`accid` AS dailytrn_accid,
     dailytrn.`prodid` AS dailytrn_prodid,
     dailytrn.`shiftid` AS dailytrn_shiftid,
     dailytrn.`dramt` AS dailytrn_dramt,
     dailytrn.`cramt` AS dailytrn_cramt,
     dailytrn.`posting` AS dailytrn_posting,
     dailytrn.`naration` AS dailytrn_naration,
     account.`ID` AS account_ID,
     account.`accname` AS account_accname
FROM
     `dailytrn` dailytrn INNER JOIN `account` account ON dailytrn.`accid` = account.`ID`
WHERE
     dailytrn.chalanno = $P{chalan_no}
 AND dailytrn.trndate = $P{trn_date}
 AND dailytrn.cramt = 0
 AND dailytrn.posting = true]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT
     config.`firmname` AS config_firmname,
     config.`address` AS config_address,
     config.`contatno` AS config_contatno,
     milkCollectionBill.`bill_date` AS milkCollectionBill_bill_date,
     milkCollectionBill.`from_date` AS milkCollectionBill_from_date,
     milkCollectionBill.`to_date` AS milkCollectionBill_to_date,
     milkCollectionBill.`prod_code` AS milkCollectionBill_prod_code,
     milkCollectionBill.`prod_name` AS milkCollectionBill_prod_name,
     milkCollectionBill.`tot_milk` AS milkCollectionBill_tot_milk,
     milkCollectionBill.`avg_FAT` AS milkCollectionBill_avg_FAT,
     milkCollectionBill.`avg_rate` AS milkCollectionBill_avg_rate,
     milkCollectionBill.`tot_milkamount` AS milkCollectionBill_tot_milkamount,
     milkCollectionBill.`tot_commision` AS milkCollectionBill_tot_commision,
     milkCollectionBill.`tot_sh_anamat` AS milkCollectionBill_tot_sh_anamat,
     milkCollectionBill.`tot_ribet` AS milkCollectionBill_tot_ribet,
     milkCollectionBill.`chiling_charges` AS milkCollectionBill_chiling_charges,
     milkCollectionBill.`travalingexp` AS milkCollectionBill_travalingexp,
     milkCollectionBill.`pashukhadya` AS milkCollectionBill_pashukhadya,
     milkCollectionBill.`mlkbilladv` AS milkCollectionBill_mlkbilladv,
     milkCollectionBill.`otherded` AS milkCollectionBill_otherded,
     milkCollectionBill.`docfee` AS milkCollectionBill_docfee,
     milkCollectionBill.`sourmlkamt` AS milkCollectionBill_sourmlkamt,
     milkCollectionBill.`amt_payable` AS milkCollectionBill_amt_payable,
     milkCollectionBill.`AddedByFK` AS milkCollectionBill_AddedByFK,
     producer.`bankid` AS producer_bankid,
     producer.`acno` AS producer_acno,
     producer.`ID` AS producer_ID
FROM
     `milkCollectionBill` milkCollectionBill INNER JOIN `producer` producer ON milkCollectionBill.`prod_code` = producer.`ID`,
     `config` config
WHERE
     producer.bankid IS null
 AND producer.acno IS null]]>
	</queryString>

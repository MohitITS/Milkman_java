	<parameter name="wherecondition" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     config.`ID` AS config_ID,
     config.`firmname` AS config_firmname,
     config.`address` AS config_address,
     config.`contatno` AS config_contatno,
     mlkCollection.`ID` AS mlkCollection_ID,
     mlkCollection.`trn_date` AS mlkCollection_trn_date,
     mlkCollection.`shift_id` AS mlkCollection_shift_id,
     mlkCollection.`prod_code` AS mlkCollection_prod_code,
     mlkCollection.`sample_no` AS mlkCollection_sample_no,
     mlkCollection.`mlkType_id` AS mlkCollection_mlkType_id,
     mlkCollection.`weight` AS mlkCollection_weight,
     mlkCollection.`fat` AS mlkCollection_fat,
     mlkCollection.`degree` AS mlkCollection_degree,
     mlkCollection.`snf` AS mlkCollection_snf,
     mlkCollection.`rate` AS mlkCollection_rate,
     mlkCollection.`smw` AS mlkCollection_smw,
     mlkCollection.`smr` AS mlkCollection_smr,
     producer.`ID` AS producer_ID,
     producer.`pro_name` AS producer_pro_name,
     producer.`prod_id` AS producer_prod_id,
     mlkCollection.`prod_id` AS mlkCollection_prod_id,
     producer.`producergroupid` AS producer_producergroupid
FROM
     `producer` producer INNER JOIN `mlkCollection` mlkCollection ON producer.`prod_id` = mlkCollection.`prod_id`,
     `config` config
WHERE
     $P!{wherecondition}
ORDER BY
     producer.producergroupid, producer.ID ASC]]>

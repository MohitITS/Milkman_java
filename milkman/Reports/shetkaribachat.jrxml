	<queryString>
		<![CDATA[SELECT
     config.`ID` AS config_ID,
     config.`firmname` AS config_firmname,
     config.`address` AS config_address,
     config.`contatno` AS config_contatno,
     prodbilldeduction.`ID` AS prodbilldeduction_ID,
     prodbilldeduction.`deddate` AS prodbilldeduction_deddate,
     prodbilldeduction.`dedfromdate` AS prodbilldeduction_dedfromdate,
     prodbilldeduction.`dedtodate` AS prodbilldeduction_dedtodate,
     prodbilldeduction.`pro_code` AS prodbilldeduction_pro_code,
     prodbilldeduction.`totamilk` AS prodbilldeduction_totamilk,
     prodbilldeduction.`amount` AS prodbilldeduction_amount,
     prodbilldeduction.`rebate` AS prodbilldeduction_rebate,
     prodbilldeduction.`shareanamat` AS prodbilldeduction_shareanamat,
     prodbilldeduction.`chilingcharges` AS prodbilldeduction_chilingcharges,
     producer.`ID` AS producer_ID,
     producer.`pro_name` AS producer_pro_name,
     producer.`prod_id` AS producer_prod_id
FROM
     `prodbilldeduction` prodbilldeduction INNER JOIN `producer` producer ON prodbilldeduction.`pro_code` = producer.`prod_id`,
     `config` config
WHERE
     $P!{wherecondition}
ORDER BY
     prodbilldeduction.dedfromdate]]>
	</queryString>
